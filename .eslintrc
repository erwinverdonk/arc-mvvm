{
    "rules": {
	 	"indent": [
            2,
            "tab"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "none"
            }
        ],
        "no-console": 1
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "globals": {
        "angular": true
    },
    "extends": "eslint:recommended"
}